[user]
	name = {{ .name | quote }}
	email = {{ .email | quote }}
[alias]
	f = fetch
	s = status --short
	l = log --oneline
	df = diff --staged
	d = diff
{{ if eq .gitUseGCM true }}
[credential]
	helper = /mnt/c/Program\\ Files/Git/mingw64/bin/git-credential-manager.exe
{{ end }}
[credential "https://dev.azure.com"]
        useHttpPath = true
